<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--#region Colors-->

    <SolidColorBrush x:Key="Level0" Color="#ff0000"/>
    <SolidColorBrush x:Key="Level1" Color="#191919"/>
    <SolidColorBrush x:Key="Level2" Color="#202020"/>
    <SolidColorBrush x:Key="Level3" Color="#2C2C2C"/>
    <SolidColorBrush x:Key="Level4" Color="#404040"/>

    <SolidColorBrush x:Key="TextLevel0" Color="#BFBFBF"/>
    <SolidColorBrush x:Key="TextLevel1" Color="#D3D3D3"/>
    <SolidColorBrush x:Key="TextLevel2" Color="#DCDCDC"/>

    <SolidColorBrush x:Key="ButtonWhite" Color="#EBEBEB"/>
    <SolidColorBrush x:Key="ButtonBlack" Color="#EBEBEB"/>

    <SolidColorBrush x:Key="Blue" Color="#1C1C94"/>
    <SolidColorBrush x:Key="Red" Color="#AF2121"/>

    <!--#endregion-->

</ResourceDictionary>